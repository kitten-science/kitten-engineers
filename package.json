{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@kitten-science/kitten-engineers",
  "version": "0.0.3",
  "description": "Kitten Engineers",
  "license": "MIT",
  "author": "Oliver Salzburg <oliver.salzburg@gmail.com>",
  "homepage": "https://github.com/kitten-science/kitten-engineers",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitten-science/kitten-engineers.git"
  },
  "bugs": {
    "url": "https://github.com/kitten-science/kitten-engineers/issues"
  },
  "type": "module",
  "exports": {
    "./*.js": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn run build:all",
    "ke:build": "vite --config vite.config.inject.js build",
    "ke:preview": "DEV_BUILD=true vite --config vite.config.userscript.js build",
    "ke:release": "MINIFY=true vite --config vite.config.userscript.js build; vite --config vite.config.userscript.js build",
    "ke:version": "node version.cjs",
    "ke:watch": "vite --config vite.config.inject.js build --watch",
    "lint": "yarn run lint:all",
    "test": "echo This project has no tests.",
    "whoami": "echo kitten-engineers"
  },
  "dependencies": {
    "@kitten-science/kitten-analysts": "0.0.1-dev-6775fe6",
    "@kitten-science/kitten-scientists": "2.0.0-beta.10-dev-ed7e6ea",
    "@oliversalzburg/js-utils": "0.2.8",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/web": "0.0.207",
    "typescript": "5.8.2",
    "vite": "6.2.0",
    "vite-plugin-css-injected-by-js": "3.5.2",
    "vite-plugin-userscript": "0.1.3"
  },
  "keywords": [
    "kittensgame"
  ],
  "engines": {
    "node": ">= 20"
  },
  "packageManager": "yarn@4.6.0"
}
